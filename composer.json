{
    "name": "19peaches/dais",
    "type": "application",
    "description": "Dais XMS Framework is a cross marketing platform that allows developers to easily create beautiful websites that include shopping, blogging, member and affiliate management without compromise.",
    "keywords": ["19peaches", "dais", "platform", "shopping", "blogging", "member management", "affiliate management"],
    "homepage": "http://dais.io",
    "license": "MIT",
    "authors": [{
        "name": "Vince Kronlein",
        "email": "vince@19peaches.com",
        "homepage": "http://19peaches.com",
        "role": "Lead Developer"
    }],
    "require": {
        "php": ">=5.5.0",
        "pimple/pimple": "~3.0",
        "foil/foil": "~0.6",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "guzzlehttp/guzzle": "6.*",
        "swiftmailer/swiftmailer": "@stable"
    },
    "require-dev": {
        "raveren/kint": "^1.0",
        "filp/whoops": "1.*"
    },
    "autoload": {
        "classmap": [
            "database/",
            "app/"
        ],
        "psr-4": {
            "Api\\": "api/",
            "Dais\\": [
                "override/",
                "peaches/"
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
